(defalias
	td_paren	(tap-dance $tdt (@lp @paren))
	td_brace	(tap-dance $tdt (@{ @brace))
	td_brack	(tap-dance $tdt ([ @brack))
	td_angle	(tap-dance $tdt (@< @angle))
	td_pcomm	(tap-dance $tdt (@rp @pcomm))
	td_ccomm	(tap-dance $tdt (/ @ccomm))
	td_squot	(tap-dance $tdt (' @squot))
	td_dquot	(tap-dance $tdt (@dq @dquot))
)